<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<!--Mybatis 설정 -->
	<settings>
		<!-- 전통적인 데이터베이스 컬럼명 형태인 A_COLUMN을 CamelCase형태의 자바 프로퍼티명 형태인 aColumn으로 자동으로 매핑하도록 함 -->
		<setting name="mapUnderscoreToCamelCase" value="true"></setting>
		<!--  파라미터에 Null 값이 있을 경우 에러 처리 -->
		<setting name="jdbcTypeForNull" value="VARCHAR"></setting>
	</settings>

	<!-- Type Aliases 설정-->
	<typeAliases>
		<typeAlias alias="UserRegVO"			type="egovframework.pubtest.login.service.UserRegVO"/>
		<typeAlias alias="infUserVO"			type="egovframework.admin.service.InfUserVO"/>
		<typeAlias alias="PubTesterMainVO" 		type="egovframework.pubtest.main.service.PubTesterMainVO" />
		<typeAlias alias="egovMap" 				type="org.egovframe.rte.psl.dataaccess.util.EgovMap" />
		<typeAlias alias="FileVO"				type="egovframework.com.cmm.service.FileVO" />
		<typeAlias alias="ComDefaultCodeVO" 	type="egovframework.com.cmm.ComDefaultCodeVO" />
		<typeAlias alias="comDefaultVO"			type="egovframework.com.cmm.ComDefaultVO" />
		
		<typeAlias alias="campaignSubmitVO"		type="egovframework.pubtest.campaign.service.CampaignSubmitVO"/>
		<typeAlias alias="campaignVO"			type="egovframework.pubtest.campaign.service.CampaignVO"/>
		
		
		<typeAlias alias="boardVO"				type="egovframework.pubtest.board.service.BoardVO"/>
		<typeAlias alias="boardDetailDTO"		type="egovframework.pubtest.board.service.BoardDetailDTO"/>
		<typeAlias alias="boardListDTO" 		type="egovframework.pubtest.board.service.BoardListDTO"/>
		
		<typeAlias alias="boardCommentDTO"		type="egovframework.pubtest.board.service.BoardCommentDTO" />
	</typeAliases>
    
</configuration>